#include<stdio.h>
#include<conio.h>
int isneg (int n);
int fact (int n);
int
main ()
{
  int n;
  printf ("Enter no.\n");
  scanf ("%d", &n);
  if (isneg (n))
    {
      printf ("Fact not possible\n");

    }
  else
    {
      printf ("The fact of no.is:%d", fact (n));
    }
}

int
isneg (int n)
{
  if (n > 0)
    return 0;
  else
    return 1;
}

int
fact (int n)
{
  int i, s;
  for (i = 0; i < n; i++)
    {
      s = n * i;
    }
  return s;
}

