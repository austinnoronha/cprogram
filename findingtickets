#include <stdio.h>

int main()
{
 int num_s,tickets,r;
 printf("please enter number of rows!!");
 scanf("%d",&r);
 printf("please enter number of seats!!");
 scanf("%d",&num_s);
 printf("please enter number of tickets!!");
 scanf("%d",&tickets);
 int n=r*num_s;
 if(n>tickets)
    printf("Opps!! tickets is less then number of seats:)");
 else
    printf("%d %d",n,tickets-n);
  
}
