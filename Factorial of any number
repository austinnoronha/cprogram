#include<stdio.h>
void main()
{
int n,i,fact=1;
printf("enter the number whose factorial is required:");
scanf("%d",&n);
if(n==0)
{
printf("\nFactorial of negative number doesn't exist");
}
else
{
for(i=1;i<=n;i++)
{
fact=fact*i;
}
}
printf("\nFactorial of %d is :%d",n,fact);
getch();
}




      
            
  
        



